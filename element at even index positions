#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "Enter number of elements: ";
    cin >> n;

    int a[n];

    cout << "Enter elements:\n";
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }

    cout << "Elements at even index positions:\n";
    for (int i = 0; i < n; i = i + 2) {
        cout << a[i] << " ";
    }

    return 0;
}

#output
Enter number of elements: 3
Enter elements:
5 6 1
Elements at even index positions:
5 1 

